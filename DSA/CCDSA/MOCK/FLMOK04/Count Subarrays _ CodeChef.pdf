  <script type="text/x-handlebars" id="problemPage">
    <main class="problem-container contest-container content" id="content-regions">
      {{#if loading}}
      <img src="https://cdn.codechef.com/Contests/images/Problem-Page.gif">
      {{else}}

      <div>
        <aside class="breadcrumbs">
          {{{partialBreadcrumbs}}}{{#if contestCode}}{{#link-to 'contestPage' contestCode}}{{contestName}}{{/link-to}}
          &nbsp;»&nbsp;{{problemName}}{{/if}}
        </aside>
      </div>

      <header>
        <div class="row collapse">
          <div class="large-12 columns">
            <h1>
              {{problemName}}
              <span>Problem Code: <strong id="problem-code">{{problemCode}}</strong></span>
              <i class="icon-add-todo right">
                {{#if isTodoWidgetAvailable}}
                {{{todoWidget}}}
                {{else}}
                {{#if isProblemSolved}}
                <img style="position: relative; top: 2px; left: 5px; height: 17px; margin-right:10px;"
                     src="https://cdn.codechef.com/sites/all/modules/codechef_tags/images/solved.png"
                     border="0" title="Solved" alt="Solved">
                {{/if}}
                {{/if}}
              </i>
              {{#if isSubmitAllowed}}
              <a class="button blue right" {{bind-attr href=problemSubmitURL }}>Submit</a>
              {{else}}
              {{#if practiceSubmissionAllowed}}
              <a class="button blue right" {{bind-attr href=practiceProblemSubmitURL }} >Submit
                (Practice)</a>
              {{/if}}
              {{/if}}
            </h1>
          <div class="clearfix"></div>
        </div>
      </header>

      <section class="content-area small-8 columns pl0">
        <div class="problem-statement" id="problem-statement">
          {{#if statement}}
            {{{convert-markdown statement}}}
            {{#if inputFormatState}}
                {{#if inputFormat}}
                    <h3>Input Format</h3>
                    {{{convert-markdown inputFormat}}}
                {{/if}}
            {{/if}}
            {{#if outputFormatState}}
                {{#if outputFormat}}
                    <h3>Output Format</h3>
                    {{{convert-markdown outputFormat}}}
                {{/if}}
            {{/if}}
            {{#if constraintsState}}
                {{#if constraints}}
                    <h3>Constraints</h3>
                    {{{convert-markdown constraints}}}
                {{/if}}
            {{/if}}
            {{#if subtasksState}}
                {{#if subtasks}}
                    <h3>Subtasks</h3>
                    {{{convert-markdown subtasks}}}
                {{/if}}
            {{/if}}
            {{#each sampleTestCases}}
                {{#if value.input}}
                    <h3>
                        Sample Input {{value.id}}
                        <i
                            title="Copy Sample Input"
                            onclick="handleCopySampleTest(event)"
                            {{bind-attr data-copy-target-id=value.sampleInputHtmlID}}
                            class="fa fa-copy test-case-copy-btn"
                        >
                        </i>
                        <i
                            class="fa fa-check-circle test-case-copy-btn"
                            style="display: none"
                            {{bind-attr id=value.sampleInputCopiedIconID}}
                        >
                        </i>
                    </h3>
                    <span {{bind-attr id=value.sampleInputHtmlID}}>
                        {{{convert-markdown value.input}}}
                    </span>
                {{/if}}
                {{#if value.output}}
                    <h3>
                        Sample Output {{value.id}}
                        <i
                            title="Copy Sample Output"
                            onclick="handleCopySampleTest(event)"
                            {{bind-attr data-copy-target-id=value.sampleOutputHtmlID}}
                            class="fa fa-copy test-case-copy-btn"
                        >
                        </i>
                        <i
                            class="fa fa-check-circle test-case-copy-btn"
                            style="display: none"
                            {{bind-attr id=value.sampleOutputCopiedIconID}}
                        >
                        </i>
                    </h3>
                    <span {{bind-attr id=value.sampleOutputHtmlID}}>
                        {{{convert-markdown value.output}}}
                    </span>
                {{/if}}
                {{#if value.explanation}}
                    <h3>Explanation</h3>
                    {{{convert-markdown value.explanation}}}
                {{/if}}
            {{/each}}
        </div>

        <!--.problem-statement-->

        <aside class="problem-info">
          {{#if isAdmin}}
          {{#each problemTimesArray}}
          <div>
            <p><b>{{key}}</b>: {{value}}</p>
          </div>
          {{/each}}
          {{/if}}
          {{#if author}}
          <p>
            <label>Author:</label>
            <span><a {{bind-attr href=problemAuthorURL}}>{{{author_html_handle}}}</a></span>
          </p>
          {{/if}} {{#if tester}}
          <p>
            <label>Tester:</label>
            <span><a {{bind-attr href=problemTesterURL}}>{{{tester_html_handle}}}</a></span>
          </p>
          {{/if}}
           {{#if editorialURL}}
              <p>
                <label>Editorial:</label>
                <span><a {{bind-attr href=editorialURL}}>{{editorialURL}}</a></span>
              </p>
          {{/if}}
          {{#if tags}}
             <p>
               <label>Tags:</label>
               <span>
               {{#if tempShowTags}}
                 {{{tags}}}
               {{else}}
                 Tags are hidden. <a {{action 'turnOnTempShowTags'}}>Show temporarily</a>
               {{/if}}
             {{#if username}}
                <hr style="margin-top: 5px; margin-bottom: 5px;"/>
                <p>
                    {{#if userZenMode}}
                        Update this setting in
                    {{else}}
                        You can hide tags from
                    {{/if}}
                    {{{userEditProfileURLAdditionalInfo}}}
                </p>
                </span>
             {{/if}}
             </p>
          {{/if}}
          {{#if problemDifficultyRating}}
              <p>
               <label>
                    <a target="_blank" href="https://blog.codechef.com/2022/02/25/exciting-updates-for-february-2022">
                        Difficulty Rating:
                    </a>
               </label>
                <span>{{problemDifficultyRating}}</span>
              </p>
          {{/if}}
          {{#if dateAdded}}
          <p>
            <label>Date Added:</label>
            <span>{{dateAdded}}</span>
          </p>
          {{/if}}
          <p>
            <label>Time Limit:</label>
            <span>{{maxTimeLimit}} secs</span>
          </p>
          <p>
            <label>Source Limit:</label>
            <span>{{sourceSizeLimit}} Bytes</span>
          </p>
          <p>
            <label>Languages:</label>
            <span>{{languagesSupported}}</span>
          </p>
        </aside>
        <!--.problem-info-->

        <div class="text-center">
          <input type="hidden" {{bind-attr value=isSubmitAllowed}}/>
          {{#if isSubmitAllowed}}
          <a class="button blue" {{bind-attr href=problemSubmitURL }}
             style="padding:0.4rem 1.5rem 0.425rem;">Submit</a>
          {{else}}
          {{#if practiceSubmissionAllowed}}
          <a class="button blue" {{bind-attr href=practiceProblemSubmitURL }}
             style="padding:0.4rem 1.5rem 0.425rem;">Submit (Practice)</a>
          {{/if}}
          {{/if}}
          {{else}}
          <div>Please click <a {{action 'reloadProblem' true}}>here</a> to reload. If problem persists,
            please report to bugs@codechef.com
          </div>
          {{/if}}
        </div>

      </section>
