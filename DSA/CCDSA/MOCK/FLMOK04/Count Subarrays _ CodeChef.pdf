    String.prototype.capitalize = function () {
        return this.charAt(0).toUpperCase() + this.slice(1);
    };

    disconnectOauth = function (event, account, username) {
        if (event != undefined) {
            event.preventDefault();
        }
        disconnect = () => {
            window.localStorage.clear();
            $.ajax({
                url: '/OAuth/' + account,
                type: 'DELETE',
                headers: {'x-csrf-token': window.csrfToken},
                success: function () {
                    window.location.reload();
                },
                error: function () {
                    invokeGenericModal({
                        message: "Couldn't complete your request. Please try again.",
                        id: account + '-error-message'
                    });
                }
            });
        }
        showDrupalConfirmBox(`Are you sure you want to disconnect ${account.capitalize()} account? <br /><br />
            For hassle free login, please make sure you have
            <a href='/users/${username}/edit#login_info' style='text-decoration: underline'> set a password</a>
            before disconnecting.`,
            disconnect
        );
    };

    connectOauth = function (event, url) {
        if (event != undefined) {
            event.preventDefault();
        }
        window.localStorage.clear();
        cAjax({
            url: url,
            type: 'POST',
            success: function (res) {
                if (res.url) {
                    window.location.href = res.url;
                }
            },
            error: function (err) {
                invokeGenericModal({
                    areAllAllowed: true,
                    message: err.message || "Couldn't complete your request. Please try again.",
                    id: 'oauth-connect-error-message'
                });
            }
        });
    };
