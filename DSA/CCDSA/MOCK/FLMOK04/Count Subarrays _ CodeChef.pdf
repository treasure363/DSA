  <script type="text/x-handlebars" id="recentActivityRow">
    <td class="date">
      <div {{bind-attr title=submission_date}}>#</div>
    </td>
    <td class="user-handle">
      <div>{{{unbound handle}}}</div>
    </td>
    <td class="problem-code">
      <div>{{{unbound code}}}</div>
    </td>
    <td class="result-code">
      <div>{{{result_code}}}</div>
    </td>
    <td class="language">
      <div {{bind-attr title=language }}>{{unbound language_short}}</div>
    </td>
  </script>
  <!--#recentActivityRow handlebars template-->

  <script type="text/x-handlebars" id="contestPageRow">
    <td class="cc-problem-name">
      {{#if is_direct_submittable}}
      <div>
        {{#if allow_viewing}}
        <span><a {{bind-attr href=submit_link title=name class=color }}>{{unbound name}}</a> {{{mark}}}</span> {{{todo_widget}}}
        {{else}}
        <span>{{unbound name}} {{{mark}}}</span> {{{todo_widget}}}
        {{/if}}
      </div>
      {{else}}
      <div>
        {{#if allow_viewing}}
        <span>{{#link-to 'contestProblemPage' meta class=color title=name }}{{unbound name}}{{/link-to}} {{{mark}}}</span> {{{todo_widget}}}
        {{else}}
        <span>{{unbound name}}{{{mark}}}</span> {{{todo_widget}}}
        {{/if}}
      </div>
      {{/if}}
    </td>
    <td>
      {{#if allow_submission}}
      <div><a {{bind-attr href=submit_link }}>{{unbound code}}</a></div>
      {{else}}
      <div>{{unbound code}}</div>
      {{/if}}
    </td>
    <td class="num">
      <div>{{{unbound score}}}</div>
    </td>
    <td class="num">
      <div><a {{bind-attr href=status_link }}>{{unbound accuracy}}</a></div>
    </td>
  </script>
  <!--#contestPageRow handlebars template-->

  <script type="text/x-handlebars" id="singleRow">
    <td>
      <div>{{{data}}}</div>
    </td>
  </script>
  <!--#singleRow handlebars template-->

  <script type="text/x-handlebars" id="contestPage">
    <main class="contest-container content">
      {{#if loading}}
      <img src="https://cdn.codechef.com/Contests/images/Loader-content.gif"> {{else}}
      <div>
        <aside class="breadcrumbs">
          {{{breadcrumbs}}}
        </aside>
      </div>
      <section class="content-area small-8 columns pl0">

        <div class="banner plr15 mb10">
          <img {{bind-attr src="banner"}} {{bind-attr alt="bannerAlt"}}>
        </div>
        {{#if refreshTimeMessage}}
        <div class="row">
          <div class="large-12 columns">
            <div class="time-zone">
              <span id="timezone_on">{{refreshTimeMessage}}</span>
            </div>
          </div>
        </div>
        {{/if}}

        {{#if isParentContest}}
        <div class="cc-text-block static-datatable div1div2">
          <div class="row collapse mb8">
            <div class="large-12 columns">
              <div class="content">
                <p class="contest-div-head">Participate in the contest based on your rating</p>
              </div>
            </div>
              {{#if data.is_older_rating_based_division_system}}
                  <div class="large-12 columns">
                      <div class="content">
                          <p class="mb10">
                            {{getOlderDivisionMessage}}
                          </p>
                      </div>
                  </div>
              {{/if}}
          </div>
          {{#if data.show_div_based_contest}}
          {{#unless isContestEnded}}
          {{#if data.user.username}}
          <div class="row collapse">
            <div class="large-12 columns">
              <div class="content">
                    <div class="contest-div-main-card--bg mb10">
                      <p class="contest-details-h6 mb10">Your rating is {{data.user_rating_div.rating}}, you can
                        participate in this contest only through {{data.user_rating_div.div.name}}</p>
                        <div class="contest-div-row">
                            <div class="contest-div-left-row">
                            <div class="contest-div-label-container">
                                <div class="contest-div-label" >{{data.user_rating_div.div.name}}</div>
                            </div>
                            <div class="contest-div-rating">Rating: &nbsp; </div>
                            <span>{{getRatingIntervalForUserDivision}}</span>
                            </div>
                            {{#link-to 'contestPage' data.user_contest_code class='div-link'}}
                            <div class="contest-div-right-row">
                                <span class="contest-div-btn__blue">Participate
                                    <i class="i-right-arrow-white-no-tail mb5"></i>
                                </span>
                            </div>
                            {{/link-to}}
                        </div>
                    </div>
              </div>
            </div>
          </div>
          {{else}}
          <div class="row collapse">
            <div class="large-12 columns">
              <div class="content">
                <div class="mb10">
                    <p>If you're a new user, please <a href="/signup">register</a>,
                      or if an existing user, please <a href="/signup">login</a>
                      to participate accordingly based on your ratings.
                    </p>
                </div>
              </div>
            </div>
          </div>
          {{/if}}
          {{/unless}}
          {{/if}}
          <div class="content">
              <div {{bind-attr class = getDivisionContainerBg}}>
                  {{#each card in divisionCards}}
                      <div {{bind-attr class = card.boxClass}}>
                          <div class="contest-div-left-row">
                              <div class="contest-div-label-container">
                                    <div class="contest-div-label">{{card.name}}</div>
                              </div>
                              <div class="contest-div-rating">Rating: &nbsp; </div>
                              <span>{{card.ratingInterval}}</span>
                          </div>
                          <div class="contest-div-right-row">
                              {{#link-to 'contestPage' card.contestCode class='div-link'}}
                                {{#if data.user.username}}
                                    <div class="contest-div-btn__no-color contest-div-btn__blue">
                                        <span>{{ card.buttonText }}<i class="i-arrow-no-tail-blue"></i></span>
                                    </div>
                                {{else}}
                                    <div class="contest-div-btn__blue">
                                        <span> {{ card.buttonText }} <i class="i-right-arrow-white-no-tail"></i></span>
                                    </div>
                                {{/if}}
                              {{/link-to}}
                          </div>
                      </div>
                  {{/each}}
              </div>
          </div>
          {{#unless data.is_older_rating_based_division_system}}
              <div class="row collapse mb20">
                <div class="large-12 columns">
                  <div class="content">
                    <p class="mb10"><strong>About Division & Rating System</strong>
                        <span class="m-label-light__yellow">Introduced DIV-4</span>
                    </p>
                    <p>We have newly introduced DIV-4 for users with <b>Rating <= 1399</b></p>
                  </div>
                </div>
              </div>
          {{/unless}}
          <div class="row collapse mb20">
            <div class="large-12 columns">
              <div class="content">
                  <div class="contest-div-more">
                    <a class="contest-div-more__link" target="_blank" href="/ratings/divisions">
                        More about Division & Rating System
                        <span class="contest-div-more__icon">
                            <i class="i-right-arrow-blue"></i>
                        </span>
                    </a>
                  </div>
              </div>
            </div>
        </div>
        </div>
        {{else}}

        {{#if false}}
        {{#if isRegistrationEnabledOrUserStartContest}}
        {{#if data.user.username}}
        {{else}}
        {{#if isContestEnded}}
        {{else}}
        <div id="contest-action"
             class="cc-text-block static-datatable announcements cc-text-block__highlighted pl40 pr40">
          <div class="text-center mb10">You need to be logged in to participate in
            contest.
          </div>
          <div class="text-center"><a style="width: 160px;" class="button blue fnone"
                                      href="/login">
            Log In
          </a> &nbsp; or &nbsp; <a style="width: 160px;" class="button green fnone"
                                   href="/signup">Create New Account</a></div>
        </div>
        {{/if}}
        {{/if}}
        {{/if}}
        {{/if}}

        {{#if isContestElementsVisible}}
        <div class="plr15">
          {{#if data.scorable_heading}}
          <b>{{data.scorable_heading}}</b>
          {{/if}}

          <div class="problem-table" id="problems-list">
            {{data-table table=this linkRouter='contestPage'}}
          </div><!--.problem-table-->
        </div>

        <div class="cc-text-block">
          {{#if unscoredProblems}}
          {{#if data.non_scorable_heading}}
          <h3>{{data.non_scorable_heading}}</h3>
          {{/if}}
          <div class="row">
            <div class="large-12 columns">
              <div class="content">
                {{#if data.non_scorable_message}}
                {{{data.non_scorable_message}}}
                {{/if}}
              </div>
            </div>
          </div>
          {{/if}}

          {{#if unscoredProblems}}
          <div class="plr15">
            <div class="problem-table" id="problems-list-2">
              {{data-table table=this.unscoredProblemsListController}}
            </div><!--.problem-table-->
          </div>
          {{/if}}
        </div>
        {{/if}}
        {{/if}}
        {{#if isRegistrationEnabledOrUserStartContest}}
        {{#if isRegistrationEnabledContest}}
        <div id="contest-register" {{bind-attr class=":cc-text-block :static-datatable :cc-visible"}}>
          <h3>Contest Registration</h3>
          <div class="row">
            <div class="large-12 columns">
              <div class="content">
                {{#if isRegistered}}
                    {{#if isContestEnded}}
                        <div>The contest is not accepting registrations anymore</div>
                    {{else}}
                        <div>You are registered for the contest</div>
                    {{/if}}
                {{else}}
                <div>You will have to register to participate in
                  this contest. Click on
                  "Register" button to proceed with your registration process.</div>
                <div>
                  <a style="display: block; margin: 0 40%;"
                     class="button fnone blue" {{action 'contestRegister'}}>Register</a>
                </div>
              </div>
              {{/if}}

            </div>
          </div>
          {{/if}}


          {{#if isFlexiTimeContest}}
          <div id="contest-start" {{bind-attr class=":cc-text-block :static-datatable :cc-visible" }}>
            <h3>Start Contest</h3>
            <div class="row">
              <div class="large-12 columns">
                <div class="content">
                  {{#if isStartTimeSet}}
                  <div>You had started the contest.</div>
                  {{else}}
                  <div>You need to start the contest to
                    access the
                    problem of this contest.
                    Click on "Start My Contest" button to begin the contest.</div>
                  <div>
                    <a style="display: block; margin: 0 34%;"
                       class="button fnone blue" {{action 'startContest'}}>Start
                    My Contest</a>
                  </div>
                  {{/if}}
                </div>
              </div>
            </div>
          </div>
          {{/if}}
          {{else}}

          {{/if}}
          {{#if data.user.username}}
          {{#if isOTPRequired}}
          <div id="contest-otp" {{bind-attr class=":cc-text-block :static-datatable :cc-visible" }}>
            <h3>Unlock contest</h3>
            <div class="row">
              <div class="large-12 columns">
                <div class="content">
                  <div>
                    <div>Please enter the OTP (One time password) in the box below, which you have received from the organiser.</div>
                    <div class="tokenForContest">
                    {{input class='text-center' type = "text" placeholder = "Enter OTP" value=oneTimePassword enter='unlockContest'}}
                    </div>
                  </div>
                  <div>
                    <a style="display: block; margin: 0 40%;"
                       class="button fnone blue" {{action 'unlockContest'}}>Unlock</a>
                  </div>
                </div>
              </div>
            </div>
          </div>
          {{/if}}
          {{/if}}

          <div id="announcements" {{bind-attr class=":cc-text-block :static-datatable :announcements:cc-visible:cc-hidden" }}>
            <h2>Announcements</h2>
            <div class="row">
              <div class="large-12 columns">
                <div class="content">{{{data.announcements}}}</div>
              </div>
            </div>
          </div><!--#announcements-->

          {{#if data.stats}}
          <div id="stats" {{bind-attr class=":cc-text-block :static-datatable :stats:cc-visible:cc-hidden" }}>
            <p class="contest-details-h2">Stats</p>
            <div class="row">
              <div class="large-12 columns">
                <div class="content">{{{data.stats}}}</div>
              </div>
            </div>
          </div><!--#stats-->
          {{/if}}

          <div id="rules" {{bind-attr class=":cc-text-block :static-datatable :rules:cc-visible:cc-hidden" }}>
            <h2>{{{data.name}}} - Details</h2>
            <div class="row">
              <div class="large-12 columns">
                <div class="content">{{{data.rules}}}</div>
              </div>
            </div>
          </div><!--#rules-->

      </section>
      <!--.content-area-->

      <aside class="sidebar small-4 columns pr0">
        <div id="timer" {{bind-attr class=":widget :pl0 :pr0 :seperate timeElapsed:cc-visible:cc-hidden" }}>
          {{{timeElapsed}}}
          <div id="contest-ended"></div>
        </div>

