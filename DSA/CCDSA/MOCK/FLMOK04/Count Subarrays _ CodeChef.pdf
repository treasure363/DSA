  var sscache = {};

  function getPage(url, data, page) {
    var cacheId = url + JSON.stringify(data);
    var cache = sscache;
    var success = function (data) {
      cache[cacheId] = data;
      $('#rankContentDiv').html(data['content']);
      var newpage = page === undefined ? 1 : (parseInt(page) + 1);
      var pageinfo = "<div align='center' class='pageinfo'> - </div>";
      if (data['max_page'] > 0) {
        pageinfo = "<div align='center' class='pageinfo'>" + newpage + " of " + data['max_page'] + "</div>";
      }
      $("#loader").html(pageinfo);

    };

    var error = function (request, errorType) {
      try {
        if (page === undefined) {
          retryButton = '<input id="retryButton" type="button" value="Retry"/>';
            $('#rankContentDiv').html(retryButton);
            $('#retryButton').click(function () {
                $('#rankContentDiv').html('<p align="center"><img src="https://cdn.codechef.com/sites/all/themes/abessive/images/ajax-loader.gif" /> Fetching content</p>');
                getPage(url, data, page);
            })
        } else {
          $("#loader").html("Oops! It couldn't load. Try again.");
        }
      } catch (e) {
      }
    };

    if (cache.hasOwnProperty(cacheId)) {
      success(cache[cacheId]);
      return;
    }

    cAjax({
      url: url,
      type: "GET",
      data: data,
      cache: false,
      timeout: 10000,
      dataType: "json",
      success: success,
      error: error
    });

  }


  function set_page(element) {
    var page;

    if (element == "prev")
      page = $("#prev_p").val();
    else if (element == "next")
      page = $("#next_p").val();

    return page;
  }

  function onload_getpage_ssubmissions(element) {
    $('#loader').html('<p><img src="https://cdn.codechef.com/misc/ajax_loader_d.gif" style="width: 120px; height:15px;"/></p>');
    var contest = "";
    if ($("#ccode").length != 0) {
      contest = '&ccode=' + $("#ccode").val();
    }

    var page = set_page(element);
    var data = "page=" + page + "&pcode=" + $("#pcode").val() + contest;
    var url = "/ssubmission/prob";
    getPage(url, data, page);
  }
</script>

</body>

